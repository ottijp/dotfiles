		"map <c-d> scrollPageDown",
		"map <c-u> scrollPageUp",
		"map j scrollDown count=3",
		"map k scrollUp count=3",
		"",
		"# Tab",
		"map x removeTab",
		"map u restoreTab",
		"map <c-h> previousTab",
		"map <c-l> nextTab",
		"map < moveTabLeft",
		"map > moveTabRight",
		"",
		"# copy URL",
		"map ,ym copyCurrentUrl format=\"[${title}](${url})\"",
		"map ,yp copyCurrentUrl format=\"${title}\\n${url}\"",
		"",
		"# omnibar",
		"map t Vomnibar.activateInNewTab",
